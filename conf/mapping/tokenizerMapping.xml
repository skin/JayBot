<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         "http://castor.org/mapping.dtd">

<mapping>
	<class name="it.nands.jaybot.plugin.tokenizer.bean.Tokens">
    	<map-to xml="tokens"/>
    
    	<field name="delimitator" type="java.lang.String">
			<bind-xml name="delimitator" node="attribute"/>
    	</field>
    	
    	<field name="tokenList" type="it.nands.jaybot.plugin.tokenizer.bean.Token" collection="arraylist">
    		<bind-xml name="token"></bind-xml>
    	</field>
    
	</class>
	
	<class name="it.nands.jaybot.plugin.tokenizer.bean.Token">
		<field name="key" type="java.lang.String">
			<bind-xml name="key"/>
    	</field>
    
		<field name="values" type="java.lang.String">
			<bind-xml name="values"/>
    	</field>
    	
    	<field name="type" type="java.lang.String">
			<bind-xml name="type"/>
    	</field>
	</class>
  
</mapping>